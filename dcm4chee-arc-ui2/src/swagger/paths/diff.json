{
  "studiesCompare": {
    "get": {
      "tags": [
        "DIFF-RS"
      ],
      "summary": "Compare Studies",
      "operationId": "CompareStudies",
      "parameters": [
        {
          "$ref": "../parameters.json#/archiveAETPathParam"
        },
        {
          "$ref": "../parameters.json#/findSCPPathParam"
        },
        {
          "$ref": "../parameters.json#/secondaryFindSCPPathParam"
        },
        {
          "$ref": "../parameters.json#/different"
        },
        {
          "$ref": "../parameters.json#/missing"
        },
        {
          "$ref": "../parameters.json#/ForceQueryByStudyUID"
        },
        {
          "$ref": "../parameters.json#/SplitStudyDateRange"
        },
        {
          "$ref": "../parameters.json#/filter"
        },
        {
          "$ref": "../parameters.json#/fuzzymatching"
        },
        {
          "$ref": "../parameters.json#/includefield"
        },
        {
          "$ref": "../parameters.json#/comparefields"
        },
        {
          "$ref": "../parameters.json#/offset"
        },
        {
          "$ref": "../parameters.json#/limit"
        },
        {
          "$ref": "../parameters.json#/priority"
        },
        {
          "$ref": "../parameters.json#/queue"
        },
        {
          "$ref": "../parameters.json#/batchID"
        }
      ],
      "responses": {
        "200": {
          "description": "different/missing studies as JSON array with DICOM JSON objects - may be empty if no study differs",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../schemas.json#/attributes"
              }
            }
          }
        },
        "202": {
          "description": "Request successfully queued for processing"
        },
        "204": {
          "description": "No matching Study found at primary C-FIND SCP"
        },
        "400": {
          "description": "Validation for one of the query parameters failed."
        },
        "404": {
          "description": "AE Title of Archive, primary or secondary C-FIND SCP not found"
        },
        "500": {
          "description": "Internal Server Error"
        },
        "502": {
          "description": "DICOM Service Exception"
        }
      }
    }
  },
  "studiesCompareCount": {
    "get": {
      "tags": [
        "DIFF-RS"
      ],
      "summary": "Compare Studies Count",
      "operationId": "CompareStudiesCount",
      "parameters": [
        {
          "$ref": "../parameters.json#/archiveAETPathParam"
        },
        {
          "$ref": "../parameters.json#/findSCPPathParam"
        },
        {
          "$ref": "../parameters.json#/secondaryFindSCPPathParam"
        },
        {
          "$ref": "../parameters.json#/different"
        },
        {
          "$ref": "../parameters.json#/missing"
        },
        {
          "$ref": "../parameters.json#/ForceQueryByStudyUID"
        },
        {
          "$ref": "../parameters.json#/SplitStudyDateRange"
        },
        {
          "$ref": "../parameters.json#/filter"
        },
        {
          "$ref": "../parameters.json#/fuzzymatching"
        },
        {
          "$ref": "../parameters.json#/includefield"
        },
        {
          "$ref": "../parameters.json#/comparefields"
        },
        {
          "$ref": "../parameters.json#/priority"
        },
        {
          "$ref": "../parameters.json#/batchID"
        }
      ],
      "responses": {
        "200": {
          "$ref": "../responses.json#/compareStudiesCount"
        },
        "400": {
          "description": "Validation for one of the query parameters failed."
        },
        "404": {
          "description": "AE Title of Archive, primary or secondary C-FIND SCP not found"
        },
        "500": {
          "description": "Internal Server Error"
        },
        "502": {
          "description": "DICOM Service Exception"
        }
      }
    }
  },
  "studiesCompareCSV": {
    "post": {
      "tags": [
        "DIFF-RS"
      ],
      "summary": "Compares Studies specified in CSV",
      "operationId": "CompareStudiesCSV",
      "parameters": [
        {
          "$ref": "../parameters.json#/archiveAETPathParam"
        },
        {
          "$ref": "../parameters.json#/findSCPPathParam"
        },
        {
          "$ref": "../parameters.json#/secondaryFindSCPPathParam"
        },
        {
          "$ref": "../parameters.json#/csvFieldPathParam"
        },
        {
          "$ref": "../parameters.json#/different"
        },
        {
          "$ref": "../parameters.json#/missing"
        },
        {
          "$ref": "../parameters.json#/includefield"
        },
        {
          "$ref": "../parameters.json#/comparefields"
        },
        {
          "$ref": "../parameters.json#/priority"
        },
        {
          "$ref": "../parameters.json#/batchID"
        }
      ],
      "requestBody": {
        "description": "CSV File containing Study Instance UIDs",
        "required": true,
        "content": {
          "text/csv": {
            "schema": {
              "type": "file",
              "format": "binary"
            }
          }
        }
      },
      "responses": {
        "202": {
          "$ref": "../responses.json#/countResults"
        },
        "204": {
          "description": "Empty file or Incorrect field position or Not a CSV file or Invalid UIDs."
        },
        "400": {
          "description": "Validation for one of the query parameters failed or CSV field for Study Instance UID is specified as less than 1"
        },
        "404": {
          "description": "No such Archive AE Title"
        },
        "500": {
          "description": "Internal Server Error"
        },
        "503": {
          "description": "Maximal number of Tasks in Queue reached"
        }
      }
    }
  }
}