        },
        {
          "$ref": "../parameters.json#/stgCmtSCPPathParam"
        }
      ],
      "responses": {
        "200": {
          "description": "Storage Commitment request accepted by external Storage Commitment SCP ",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../schemas/stgcmtscu.schema.json"
              }
            }
          }
        },
        "404": {
          "description": "No such Archive AE Title, Study or external AE Title"
        },
        "500": {
          "description": "Internal Server Error"
        },
        "502": {
          "description": "Storage Commitment SCP Failure",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../schemas/stgcmtscu.schema.json"
              }
            }
          }
        }
      }
    }
  },
  "delete": {
    "delete": {
      "tags": [
        "IOCM-RS"
      ],
      "summary": "Delete Study Permanently",
      "operationId": "DeleteStudy",
      "parameters": [
        {
          "$ref": "../parameters.json#/archiveAETPathParam"
        },
        {
          "$ref": "../parameters.json#/studyPathParam"
        }
      ],
      "responses": {
        "204": {
          "description": "successful operation"
        },
        "403": {
          "description": "Study is not empty."
        },
        "404": {
          "description": "No such Application Entity or Study"
        },
        "500": {
          "description": "Internal Server Error."
        }
      }
    }
  },
  "copy": {
    "post": {
      "tags": [
        "IOCM-RS"
      ],
      "summary": "Copy Instances to Study",
      "operationId": "CopyInstances",
      "parameters": [
        {
          "$ref": "../parameters.json#/archiveAETPathParam"
        },
        {
          "$ref": "../parameters.json#/studyPathParam"
        },
        {
          "$ref": "../parameters.json#/filter"
        }
      ],
      "requestBody": {
        "$ref": "../requestBodies.json#/HierarchicalSOPInstanceReference"
      },
      "responses": {
        "200": {
          "description": "All referenced instances copied to target study",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../schemas.json#/attributes"
              }
            }
          }
        },
        "202": {
          "description": "This indicates that some Instances were copied to target study, but failures exist for others.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../schemas.json#/attributes"
              }
            }
          }
        },
        "400": {
          "description": "JSON request body not well formed or Missing Study Instance UID in request body"
        },
        "404": {
          "description": "No such Application Entity, Study."
        },
        "409": {
          "description": "Copy Instances to target study failed for all.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../schemas.json#/attributes"
              }
            }
          }
        },
        "500": {
          "description": "Internal Server Error."
        }
      }
    }
  },
