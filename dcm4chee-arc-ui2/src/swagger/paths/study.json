{
  "reject": {
    "post": {
      "tags": [
        "IOCM-RS"
      ],
      "summary": "Reject Study",
      "operationId": "RejectStudy",
      "parameters": [
        {
          "$ref": "../parameters.json#/archiveAETPathParam"
        },
        {
          "$ref": "../parameters.json#/studyPathParam"
        },
        {
          "$ref": "../parameters.json#/codeValuePathParam"
        },
        {
          "$ref": "../parameters.json#/codingSchemeDesignatorPathParam"
        },
        {
          "$ref": "../parameters.json#/queue"
        },
        {
          "$ref": "../parameters.json#/batchID"
        }
      ],
      "responses": {
        "200": {
          "$ref": "../responses.json#/countResults"
        },
        "202": {
          "description": "Rejection of Study queued"
        },
        "404": {
          "description": "No such Archive AE Title, Study or Rejection Note"
        },
        "500": {
          "description": "Internal Server Error"
        },
        "503": {
          "description": "Maximal number of Tasks in Queue reached"
        }
      }
    }
  },
  "updateStudyAccessCtrlID": {
    "post": {
      "tags": [
        "ACCESS-RS"
      ],
      "summary": "Update Study Access Control ID",
      "operationId": "UpdateStudyAccessControlID",
      "parameters": [
        {
          "$ref": "../parameters.json#/archiveAETPathParam"
        },
        {
          "$ref": "../parameters.json#/studyPathParam"
        },
        {
          "$ref": "../parameters.json#/accessControlIDPathParam"
        }
      ],
      "responses": {
        "204": {
          "description": "successful operation"
        },
        "404": {
          "description": "No such Archive AE Title or Study"
        },
        "500": {
          "description": "Internal Server Error."
        }
      }
    }
  },
  "importImpaxReport": {
    "post": {
      "tags": [
        "IMPAX-REPORT-RS"
      ],
      "summary": "Import Reports of Study",
      "operationId": "importReportsOfStudy",
      "parameters": [
        {
          "$ref": "../parameters.json#/archiveAETPathParam"
        },
        {
          "$ref": "../parameters.json#/studyPathParam"
        }
      ],
      "responses": {
        "200": {
          "description": "This indicates that all reports returned by the Impax Report Service were successfully stored.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../schemas.json#/attributes"
              }
            }
          }
        },
        "202": {
          "description": "This indicates that some reports returned by the Impax Report Service were stored, but failures exist for others.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../schemas.json#/attributes"
              }
            }
          }
        },
        "404": {
          "description": "no such Archive AET or Study"
        },
        "409": {
          "description": "Impax Report Service returns no report for the study"
        },
        "500": {
          "description": "Internal Server Error."
        },
        "502": {
          "description": "Connection to remote side fails"
        }
      }
    }
  },
  "ian": {
    "post": {
      "tags": [
        "IAN-RS"
      ],
      "summary": "Send Instance Availability Notification of Study to external Instance Availability Notification SCP",
      "operationId": "SendInstanceAvailabilityNotificationOfStudyExternal",
      "parameters": [
        {
          "$ref": "../parameters.json#/archiveAETPathParam"
        },
        {
          "$ref": "../parameters.json#/studyPathParam"
        },
        {
          "$ref": "../parameters.json#/ianSCPPathParam"
        }
      ],
      "responses": {
        "200": {
          "description": "Instance Availability Notification request accepted by external Instance Availability Notification SCP ",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../schemas/ianscu.schema.json"
              }
            }
          }
        },
        "404": {
          "description": "No such Archive AE Title, Study or Instance Availability Notification SCP AE Title"
        },
        "500": {
          "description": "Internal Server Error."
        },
        "502": {
          "description": "Instance Availability Notification SCP Failure",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "../schemas/ianscu.schema.json"
              }
            }
          }
        }
      }
    }
  },
  "stgCmt": {
    "post": {
      "tags": [
