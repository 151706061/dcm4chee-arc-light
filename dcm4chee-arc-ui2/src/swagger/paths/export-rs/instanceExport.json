        "$ref": "../../parameters.json#/archiveAETPathParam"
      },
      {
        "$ref": "../../parameters.json#/studyPathParam"
      },
      {
        "$ref": "../../parameters.json#/seriesPathParam"
      },
      {
        "$ref": "../../parameters.json#/instancePathParam"
      },
      {
        "$ref": "../../parameters.json#/ExporterIDPathParam"
