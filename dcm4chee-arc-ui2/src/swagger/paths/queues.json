  "cancel": {
    "post": {
      "tags": [
        "QUEUE-RS"
      ],
      "summary": "Cancel Tasks in Queue",
      "operationId": "cancelTasks",
      "parameters": [
        {
          "$ref": "../parameters.json#/queuePathParam"
        },
        {
          "$ref": "../parameters.json#/dicomDeviceName"
        },
        {
          "$ref": "../parameters.json#/cancelTaskStatus"
        },
        {
          "$ref": "../parameters.json#/batchID"
        },
        {
          "$ref": "../parameters.json#/JMSMessageID"
        },
        {
          "$ref": "../parameters.json#/createdTime"
        },
        {
          "$ref": "../parameters.json#/updatedTime"
        }
      ],
      "responses": {
        "200": {
          "$ref": "../responses.json#/countResults"
        },
        "400": {
          "description": "Cannot cancel tasks with status CANCELED, COMPLETED, WARNING or FAILED or Validation for one of the query parameters failed."
        },
        "404": {
          "description": "Queue not found"
        },
        "409": {
          "description": "Invalid Queue Message status"
        },
        "500": {
          "description": "Internal Server Error"
        }
      }
    }
  },
  "cancelQueueMsg": {
    "post": {
      "tags": [
        "QUEUE-RS"
      ],
      "summary": "Cancel Task",
      "operationId": "cancelTask",
      "parameters": [
        {
          "$ref": "../parameters.json#/queuePathParam"
        },
        {
          "$ref": "../parameters.json#/taskIDPathParam"
        }
      ],
      "responses": {
        "204": {
          "description": "successful operation"
        },
        "404": {
          "description": "No such queue or task"
        },
        "409": {
          "description": "tasks with status CANCELED, COMPLETED, WARNING or FAILED cannot be canceled"
        },
        "500": {
          "description": "Internal Server Error"
        }
      }
    }
  },
  "reschedule": {
    "post": {
      "tags": [
        "QUEUE-RS"
      ],
      "summary": "Reschedule Tasks in Queue",
      "operationId": "rescheduleTasks",
      "parameters": [
        {
          "$ref": "../parameters.json#/queuePathParam"
        },
        {
          "$ref": "../parameters.json#/dicomDeviceName"
        },
        {
          "$ref": "../parameters.json#/newDeviceName"
        },
        {
          "$ref": "../parameters.json#/queueMsgStatus"
        },
        {
          "$ref": "../parameters.json#/batchID"
        },
        {
          "$ref": "../parameters.json#/JMSMessageID"
        },
        {
          "$ref": "../parameters.json#/createdTime"
        },
        {
          "$ref": "../parameters.json#/updatedTime"
        }
      ],
      "responses": {
        "200": {
          "$ref": "../responses.json#/countResults"
        },
        "202": {
          "$ref": "../responses.json#/accepted"
        },
        "400": {
          "description": "Missing Queue Message Status or Validation for one of the query parameters failed"
        },
        "404": {
          "description": "Queue not found"
        },
        "409": {
          "$ref": "../responses.json#/failed"
        },
        "500": {
          "description": "Internal Server Error"
        }
      }
    }
  },
  "rescheduleQueueMsg": {
    "post": {
      "tags": [
        "QUEUE-RS"
      ],
      "summary": "Reschedule Task",
      "operationId": "rescheduleTask",
      "parameters": [
        {
          "$ref": "../parameters.json#/queuePathParam"
        },
        {
          "$ref": "../parameters.json#/taskIDPathParam"
        },
        {
          "$ref": "../parameters.json#/newDeviceName"
        }
      ],
      "responses": {
        "204": {
          "description": "successful operation"
        },
        "404": {
          "description": "No such queue or task"
        },
        "409": {
          "description": "Validation of task referring an Archive AE or HL7 App failed. Configuration not available on new device."
        },
        "500": {
          "description": "Internal Server Error"
        }
      }
    }
  },
