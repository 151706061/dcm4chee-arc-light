          "options": {
            "browserTarget": "dcm4chee-arc-ui2:build",
            "crossOrigin": "use-credentials"
          },
          "configurations": {
            "production": {
              "browserTarget": "dcm4chee-arc-ui2:build:production"
            },
            "sq": {
              "browserTarget": "dcm4chee-arc-ui2:build:sq"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dcm4chee-arc-ui2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "dcm4chee-arc-ui2:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dcm4chee-arc-ui2:serve:production"
            }
          }
        }
